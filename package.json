{
	"name": "askoverflow",
	"displayName": "AskOverflow",
	"description": "Ask and search questions on Stackoverflow directly from inside VS Code",
	"version": "1.0.0",
	"publisher": "fabio-sabbion",
	"author": {
		"name": "Fabio Sabbion"
	},
	"icon": "media/askoverflow_icon.png",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/sfabio01/askoverflow"
	},
	"activationEvents": [
		"onCommand:askoverflow.insertCode",
		"onView:askoverflow-sidebar"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "askoverflow-sidebar-view",
					"title": "AskOverflow",
					"icon": "media/sidebar_icon.svg"
				}
			]
		},
		"views": {
			"askoverflow-sidebar-view": [
				{
					"type": "webview",
					"id": "askoverflow-sidebar",
					"name": "AskOverflow",
					"icon": "media/icon.svg",
					"contextualTitle": "AskOverflow"
				}
			]
		},
		"commands": [
			{
				"command": "askoverflow.insertCode",
				"category": "AskOverflow",
				"title": "Insert code from current selection"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
		"package": "rollup -c && webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"node-fetch": "^3.0.0",
		"polka": "^0.5.2"
	},
	"devDependencies": {
		"vscode": "*",
		"@rollup/plugin-commonjs": "^20.0.0",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@rollup/plugin-typescript": "^8.2.5",
		"@tsconfig/svelte": "^2.0.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/polka": "^0.5.3",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"concurrently": "^6.2.1",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"rollup": "^2.56.3",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.42.6",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"bugs": {
		"url": "https://github.com/sfabio01/askoverflow",
		"email": "fabiosabbion.apps@gmail.com"
	},
	"homepage": "https://github.com/sfabio01/askoverflow/blob/main/README.md"
}